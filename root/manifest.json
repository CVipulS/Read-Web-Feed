{
    "name": "Read Web feed",
    "version": "0.0.0.0",
    "manifest_version": 3,
    "description": "browser XML syndication feed capability",
    "icons": {
        "16": "icons/icon16x16.png",
        "32": "icons/icon32x32.png",
        "48": "icons/icon48x48.png",
        "128": "icons/icon128x128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "webNavigation",
        "tabs",
        "scripting",
        "sidePanel"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {},
    "side_panel": {
        "default_path": "sidepanel.xhtml"
    },
    "incognito": "not_allowed"
}